{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "camelcase":"off",
        "no-continue":"off",
        "no-plusplus":"off",
        "guard-for-in":"off",
        "no-restricted-syntax":"off",
        "no-undef":"off",
        "eol-last":"off"
    }
}
